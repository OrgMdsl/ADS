<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Business.Entity.Map.ChamadoHistorico" table="ChamadoHistorico" schema="dbo" catalog="ProjetoADS5" optimistic-lock="version">
        <id name="idChamadoHistorico" type="int">
            <column name="id_chamado_historico" />
            <generator class="identity" />
        </id>
        <many-to-one name="chamado" class="Business.Entity.Map.Chamado" fetch="select">
            <column name="id_chamado" />
        </many-to-one>
        <many-to-one name="chamadoTipoChamado" class="Business.Entity.Map.ChamadoTipoChamado" fetch="select">
            <column name="id_tipo_chamado" />
        </many-to-one>
        <many-to-one name="coreArquivo" class="Business.Entity.Map.CoreArquivo" fetch="select">
            <column name="id_arquivo" />
        </many-to-one>
        <many-to-one name="coreGenericoItemByIdChamadoStatus" class="Business.Entity.Map.CoreGenericoItem" fetch="select">
            <column name="id_chamado_status" />
        </many-to-one>
        <many-to-one name="coreGenericoItemByIdChamadoNivel" class="Business.Entity.Map.CoreGenericoItem" fetch="select">
            <column name="id_chamado_nivel" />
        </many-to-one>
        <many-to-one name="coreGenericoItemByIdChamadoPrioridade" class="Business.Entity.Map.CoreGenericoItem" fetch="select">
            <column name="id_chamado_prioridade" />
        </many-to-one>
        <many-to-one name="usuarioByIdUsuarioSolicitante" class="Business.Entity.Map.Usuario" fetch="select">
            <column name="id_usuario_solicitante" />
        </many-to-one>
        <many-to-one name="usuarioByIdUsuarioAtendente" class="Business.Entity.Map.Usuario" fetch="select">
            <column name="id_usuario_atendente" />
        </many-to-one>
        <property name="data" type="timestamp">
            <column name="data" length="23" />
        </property>
    </class>
</hibernate-mapping>
