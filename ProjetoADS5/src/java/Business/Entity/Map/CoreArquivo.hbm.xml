<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Business.Entity.Map.CoreArquivo" table="CoreArquivo" schema="dbo" catalog="ProjetoADS5" optimistic-lock="version">
        <id name="idArquivo" type="int">
            <column name="id_arquivo" />
            <generator class="identity" />
        </id>
        <property name="nome" type="string">
            <column name="nome" length="200" />
        </property>
        <property name="extensao" type="string">
            <column name="extensao" length="10" />
        </property>
        <property name="arquivo" type="binary">
            <column name="arquivo" />
        </property>
        <property name="url" type="string">
            <column name="url" />
        </property>
        <set name="chamadoHistoricos" table="ChamadoHistorico" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="id_arquivo" />
            </key>
            <one-to-many class="Business.Entity.Map.ChamadoHistorico" />
        </set>
    </class>
</hibernate-mapping>
