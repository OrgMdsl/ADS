<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Business.Entity.Map.EnderecoPessoa" table="EnderecoPessoa" schema="dbo" catalog="ProjetoADS5" optimistic-lock="version">
        <id name="idEnderecoPessoa" type="int">
            <column name="id_endereco_pessoa" />
            <generator class="identity" />
        </id>
        <many-to-one name="endereco" class="Business.Entity.Map.Endereco" fetch="select">
            <column name="id_endereco" />
        </many-to-one>
        <many-to-one name="pessoa" class="Business.Entity.Map.Pessoa" fetch="select">
            <column name="id_pessoa" />
        </many-to-one>
    </class>
</hibernate-mapping>
