<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="Business.Entity.Map.PessoaEmpresaEndereco" table="Pessoa_Empresa_Endereco" schema="dbo" catalog="ProjetoADS5" optimistic-lock="version">
        <id name="id" type="integer">
            <column name="id_pessoa_empresa_endereco" />
            <generator class="identity" />
        </id>
        <many-to-one name="pessoaEmpresa" class="Business.Entity.Map.PessoaEmpresa" fetch="select">
            <column name="id_pessoa_empresa" />
        </many-to-one>
        <property name="cep" type="string">
            <column name="cep" length="50" />
        </property>
        <property name="numero" type="string">
            <column name="numero" length="50" />
        </property>
        <property name="dataCadastro" type="timestamp">
            <column name="data_cadastro" length="23" />
        </property>
        <property name="ativo" type="boolean">
            <column name="ativo" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
