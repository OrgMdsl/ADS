<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Business.Entity.Map.UsuarioPersonalizacao" table="UsuarioPersonalizacao" schema="dbo" catalog="ProjetoADS5" optimistic-lock="version">
        <id name="idPersonalizacao" type="int">
            <column name="id_personalizacao" />
            <generator class="identity" />
        </id>
        <many-to-one name="usuario" class="Business.Entity.Map.Usuario" fetch="select">
            <column name="id_usuario" />
        </many-to-one>
    </class>
</hibernate-mapping>
