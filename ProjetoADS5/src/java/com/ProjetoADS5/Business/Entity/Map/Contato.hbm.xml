<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class catalog="ProjetoADS5" name="com.ProjetoADS5.Business.Entity.Map.Contato" optimistic-lock="version" schema="dbo" table="Contato">
    <id name="id" type="integer">
      <column name="id_contato"/>
      <generator class="identity"/>
    </id>
    <property name="nome" type="string">
      <column length="50" name="nome"/>
    </property>
    <property name="telefone" type="string">
      <column length="50" name="telefone"/>
    </property>
    <property name="email" type="string">
      <column length="100" name="email"/>
    </property>
    <property name="dataCadastro" type="timestamp">
      <column length="23" name="data_cadastro"/>
    </property>
    <property name="ativo" type="boolean">
      <column name="ativo" not-null="true"/>
    </property>
    <bag fetch="select" inverse="true" lazy="true" name="pessoaEmpresaContatos" table="Pessoa_Empresa_Contato">
      <key>
        <column name="id_contato"/>
      </key>
      <one-to-many class="com.ProjetoADS5.Business.Entity.Map.PessoaEmpresaContato"/>
    </bag>
  </class>
</hibernate-mapping>
