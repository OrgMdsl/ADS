<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class catalog="ProjetoADS5" name="com.ProjetoADS5.Business.Entity.Map.Contrato" optimistic-lock="version" schema="dbo" table="Contrato">
    <id name="id" type="integer">
      <column name="id_contrato"/>
      <generator class="identity"/>
    </id>
    <many-to-one class="com.ProjetoADS5.Business.Entity.Map.Cliente" fetch="select" name="cliente">
      <column name="id_cliente"/>
    </many-to-one>
    <property name="dataCadastro" type="timestamp">
      <column length="23" name="data_cadastro"/>
    </property>
    <property name="ativo" type="boolean">
      <column name="ativo" not-null="true"/>
    </property>
    <bag fetch="select" inverse="true" lazy="true" name="contratoMaquinas" table="Contrato_Maquina">
      <key>
        <column name="id_contrato"/>
      </key>
      <one-to-many class="com.ProjetoADS5.Business.Entity.Map.ContratoMaquina"/>
    </bag>
  </class>
</hibernate-mapping>
