<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class catalog="GerenciadorConteudo2" name="com.ProjetoADS5.Business.Entity.Map.GenericoItem" optimistic-lock="version" schema="dbo" table="Generico_Item">
    <id name="id" type="integer">
      <column name="id_generico_item"/>
      <generator class="identity"/>
    </id>
    <many-to-one cascade="all" class="com.ProjetoADS5.Business.Entity.Map.Generico" fetch="select" name="generico">
      <column name="id_generico"/>
    </many-to-one>
    <property name="descricao" type="text">
      <column name="descricao"/>
    </property>
    <property name="titulo" type="string">
      <column name="titulo"/>
    </property>
    <property name="dataCadastro" type="timestamp">
      <column name="data_cadastro"/>
    </property>
    <property name="ativo" type="boolean">
      <column name="ativo" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
