<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class catalog="TrabADS5" name="com.ProjetoADS5.Business.Entity.Map.GenericoItem" optimistic-lock="version" schema="dbo" table="Generico_Item">
    <id name="id" type="integer">
      <column name="id_generico_item"/>
      <generator class="identity"/>
    </id>
    <many-to-one class="com.ProjetoADS5.Business.Entity.Map.Generico" fetch="select" name="generico">
      <column name="id_generico"/>
    </many-to-one>
    <property name="descricao" type="string">
      <column length="300" name="descricao"/>
    </property>
    <property name="sigla" type="string">
      <column length="5" name="sigla"/>
    </property>
    <property name="dataCadastro" type="timestamp">
      <column name="data_cadastro"/>
    </property>
    <property name="ativo" type="boolean">
      <column name="ativo" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
