<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class catalog="ProjetoADS5" name="com.ProjetoADS5.Business.Entity.Map.PessoaEmpresaEndereco" optimistic-lock="version" schema="dbo" table="Pessoa_Empresa_Endereco">
    <id name="id" type="integer">
      <column name="id_pessoa_empresa_endereco"/>
      <generator class="identity"/>
    </id>
    <many-to-one class="com.ProjetoADS5.Business.Entity.Map.PessoaEmpresa" fetch="select" name="pessoaEmpresa">
      <column name="id_pessoa_empresa"/>
    </many-to-one>
    <property name="cep" type="string">
      <column length="50" name="cep"/>
    </property>
    <property name="numero" type="string">
      <column length="50" name="numero"/>
    </property>
    <property name="dataCadastro" type="timestamp">
      <column length="23" name="data_cadastro"/>
    </property>
    <property name="ativo" type="boolean">
      <column name="ativo" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
